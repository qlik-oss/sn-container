{
  "name": "@nebula.js/sn-container",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.2",
  "description": "An agnostic container",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-container.js",
    "sn-container.qext",
    "sn-container-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-container.js",
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:watch": "node ./scripts/build.js --ext --mode development -w",
    "lint": "eslint --ext .ts,.js src/",
    "start": "nebula serve",
    "sense": "nebula sense",
    "types:check": "tsc --noEmit",
    "types:watch": "tsc --noEmit -w",
    "test:unit": "jest --env=jsdom --runInBand src/",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "format": "prettier --write .",
    "release:major": "git pull && yarn version --message 'chore: v%s' --major && git push --follow-tags",
    "release:minor": "git pull && yarn version --message 'chore: v%s' --minor && git push --follow-tags",
    "release:patch": "git pull && yarn version --message 'chore: v%s' --patch && git push --follow-tags",
    "release:pre": "git pull && yarn version --prerelease --message 'chore: v%s' --preid='pre' && git push --follow-tags",
    "version": "yarn spec && git add api-specifications",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@jest/types": "27.5.1",
    "@nebula.js/cli": "3.3.0",
    "@nebula.js/cli-build": "3.3.0",
    "@nebula.js/cli-sense": "3.3.0",
    "@nebula.js/cli-serve": "3.3.0",
    "@rollup/plugin-typescript": "9.0.2",
    "@types/enzyme": "3.10.18",
    "@types/jest": "27.5.2",
    "@types/node": "18.19.18",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "@types/requirejs": "2.1.37",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "babel-jest": "27.5.1",
    "enzyme": "3.11.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-mocha": "10.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "10.1.0",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.8.8",
    "pretty-quick": "3.3.1",
    "qlik-chart-modules": "0.67.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
    "typescript": "4.9.5",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.15.10",
    "xmldom": "0.6.0"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.3.0"
  }
}
