{
  "name": "@nebula.js/sn-container",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.2",
  "description": "An agnostic container",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "files": [
    "api-specifications",
    "core",
    "dist",
    "sn-container.js",
    "sn-container.qext",
    "sn-container-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-container.js",
  "module": "dist/sn-container.esm.js",
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:watch": "node ./scripts/build.js --ext --mode development -w",
    "lint": "eslint --ext .ts,.js src/",
    "start": "nebula serve",
    "sense": "nebula sense",
    "types:check": "tsc --noEmit -w",
    "test:unit": "jest --env=jsdom --runInBand src/",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "format": "prettier --write .",
    "major-release": "git pull && yarn version --message 'chore: v%s' --major && git push --follow-tags",
    "minor-release": "git pull && yarn version --message 'chore: v%s' --minor && git push --follow-tags",
    "patch-release": "git pull && yarn version --message 'chore: v%s' --patch && git push --follow-tags",
    "pre-release": "git pull && yarn version --prerelease --message 'chore: v%s' --preid='pre' && git push --follow-tags",
    "version": "yarn spec && git add api-specifications",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@jest/types": "27.5.1",
    "@nebula.js/cli": "3.0.2",
    "@nebula.js/cli-build": "3.0.2",
    "@nebula.js/cli-sense": "3.0.2",
    "@nebula.js/cli-serve": "3.0.2",
    "@rollup/plugin-typescript": "8.5.0",
    "@types/enzyme": "3.10.12",
    "@types/jest": "27.5.2",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/requirejs": "2.1.34",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "axios": "0.27.2",
    "babel-jest": "27.5.1",
    "enzyme": "3.11.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "qlik-chart-modules": "0.13.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
    "typescript": "4.8.4",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.5"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.3.0"
  }
}
